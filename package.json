{
    "devDependencies": {
        "@types/jasmine": "^4.0.3",
        "@typescript-eslint/eslint-plugin": "^5.30.6",
        "@typescript-eslint/parser": "^5.30.6",
        "esbuild": "^0.14.54",
        "eslint": "^8.19.0",
        "eslint-config-google": "^0.14.0",
        "jasmine": "^4.3.0",
        "node-fetch": "^2.6.7",
        "ts-node": "^10.9.1",
        "typescript": "^4.5.4"
    },
    "scripts": {
        "tsc": "tsc --build --verbose",
        "build-prod": "sh build.sh",
        "build": "sh build.sh --dev --no_tsc",
        "lint": "eslint . --ext .js --ext .ts",
        "test": "node ts-port/testing.js"
    },
    "dependencies": {
        "decimal.js": "^10.4.3"
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
            "ecmaVersion": "latest",
            "sourceType": "module"
        },
        "plugins": [
            "@typescript-eslint"
        ],
        "rules": {
            "quotes": ["error", "double", {"avoidEscape": true}],
            "require-jsdoc": "off",
            "valid-jsdoc": "off",
            "max-len": ["error", {"code": 150}],
            "indent": ["error", 4],
            "linebreak-style": 0
        },
        "ignorePatterns": ["ts-port/symtype.js", "ts-port/testing.js"]
    }
}